{
  "name": "minimalistic-react-redux-boilerplate",
  "version": "0.0.1",
  "description": "Minimalistic starter kit for React and Redux",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "build": "set NODE_ENV=production && webpack --config webpack/config-prod.babel.js",
    "open:src": "babel-node webpack/start.js",
    "lint": "node_modules/.bin/esw config.* src webpack",
    "lint:watch": "npm run lint -- --watch"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "immutable",
    "eslint",
    "ES6",
    "sass"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "copy-webpack-plugin": "^4.0.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "open": "^0.0.5",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-runtime": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-stage-0": "^6.16.0",
    "react-router-redux": "^4.0.7",
    "redux-immutable-state-invariant": "^1.2.4",
    "eslint": "^3.11.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "^0.9.6",
    "node-sass": "^4.1.1",
    "file-loader": "^0.9.0",
    "postcss-loader": "^1.2.1",
    "css-loader": "^0.26.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "0.5.7",
    "npm-run-all": "^4.0.0"
  }
}
